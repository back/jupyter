   "source": [
    "s='hellohelloworld'\n",
    "a=set(s)\n",
    "b=sorted(a)\n",
    "s=\"-\"+s+'-'\n",
    "\n",
    "while b:\n",
    "    c=b.pop()\n",
    "    sp = [i for i in s.split(c) if i]\n",
    "    if b:\n",
    "        s = s.replace(c, '')\n",
    "    print(c,sp, s)\n"
   ]
